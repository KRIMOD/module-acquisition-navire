{
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react",
    "checkJs": true,
    "paths": {
      "@/components/*": ["components/*"],
      "@/data/*": ["data/*"],
      "@/layouts/*": ["layouts/*"],
      "@/lib/*": ["lib/*"],
      "@/styles/*": ["styles/*"]
    }
  },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}
